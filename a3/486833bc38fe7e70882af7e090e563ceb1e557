<span style=3D"color: #006f00; font-weight: bold;">new</span> code:

``<span style=3D"color: #242521;">`ocaml</span>

<span style=3D"color: #4f677f; font-style: italic;">(** Code for orgchart.p=
roto *)</span>

<span style=3D"color: #8f6f4a; font-style: italic;">(* </span><span style=
=3D"color: #8f6f4a; font-style: italic;">generated from "orgchart.proto", d=
o not edit</span><span style=3D"color: #8f6f4a; font-style: italic;"> *)</s=
pan>



<span style=3D"color: #4f677f; font-style: italic;">(** </span><span style=
=3D"color: #804f60; font-style: italic;">{2</span><span style=3D"color: #4f=
677f; font-style: italic;"> Types</span><span style=3D"color: #804f60; font=
-style: italic;">}</span><span style=3D"color: #4f677f; font-style: italic;=
"> *)</span>

<span style=3D"color: #242521; font-weight: bold;">type</span> <span style=
=3D"color: #444fcf; font-weight: bold;">person</span> =3D <span style=3D"co=
lor: #006f00; font-weight: bold;">private</span> {
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> _presen=
ce: <span style=3D"color: #557400; font-weight: bold;">Pbrt.Bitfield.</span=
>t; <span style=3D"color: #4f677f; font-style: italic;">(** presence for 2 =
fields *)</span>
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> name : =
string;
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> age : i=
nt64;
}

<span style=3D"color: #242521; font-weight: bold;">type</span> <span style=
=3D"color: #444fcf; font-weight: bold;">store</span> =3D <span style=3D"col=
or: #006f00; font-weight: bold;">private</span> {
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> _presen=
ce: <span style=3D"color: #557400; font-weight: bold;">Pbrt.Bitfield.</span=
>t; <span style=3D"color: #4f677f; font-style: italic;">(** presence for 1 =
fields *)</span>
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> address=
 : string;
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> employe=
es : person list;
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> clients=
 : person list;
}

<span style=3D"color: #242521; font-weight: bold;">type</span> <span style=
=3D"color: #444fcf; font-weight: bold;">company</span> =3D <span style=3D"c=
olor: #006f00; font-weight: bold;">private</span> {
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> _presen=
ce: <span style=3D"color: #557400; font-weight: bold;">Pbrt.Bitfield.</span=
>t; <span style=3D"color: #4f677f; font-style: italic;">(** presence for 1 =
fields *)</span>
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> name : =
string;
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> stores =
: store list;
  <span style=3D"color: #006f00; font-weight: bold;">mutable</span> subsidi=
aries : company list;
}


<span style=3D"color: #4f677f; font-style: italic;">(** </span><span style=
=3D"color: #804f60; font-style: italic;">{2</span><span style=3D"color: #4f=
677f; font-style: italic;"> Basic values</span><span style=3D"color: #804f6=
0; font-style: italic;">}</span><span style=3D"color: #4f677f; font-style: =
italic;"> *)</span>

<span style=3D"color: #242521; font-weight: bold;">val</span> <span style=
=3D"color: #a7601f;">default_person</span> : unit -&gt; person=20
<span style=3D"color: #4f677f; font-style: italic;">(** </span><span style=
=3D"color: #804f60; font-style: italic;">[</span>default_person ()<span sty=
le=3D"color: #804f60; font-style: italic;">]</span><span style=3D"color: #4=
f677f; font-style: italic;"> is a new empty value for type </span><span sty=
le=3D"color: #804f60; font-style: italic;">[</span>person<span style=3D"col=
or: #804f60; font-style: italic;">]</span><span style=3D"color: #4f677f; fo=
nt-style: italic;"> *)</span>

<span style=3D"color: #242521; font-weight: bold;">val</span> <span style=
=3D"color: #a7601f;">default_store</span> : unit -&gt; store=20
<span style=3D"color: #4f677f; font-style: italic;">(** </span><span style=
=3D"color: #804f60; font-style: italic;">[</span>default_store ()<span styl=
e=3D"color: #804f60; font-style: italic;">]</span><span style=3D"color: #4f=
677f; font-style: italic;"> is a new empty value for type </span><span styl=
e=3D"color: #804f60; font-style: italic;">[</span>store<span style=3D"color=
